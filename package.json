{
    "name": "postablereader",
    "version": "1.0.0",
    "description": "POS Machine Reader by Electron for SaladLab",
    "main": "bootstrapper.js",
    "scripts": {
        "start": "electron .",
        "build-mac": "electron-packager ./ --out ./build POSTableReader --platform=darwin --asar=true",
        "build-win": "electron-packager ./ --out ./build POSTableReader --platform=win32 --asar=true"
    },
    "repository": "https://github.com/hlhr202/POSTableReader",
    "keywords": [
        "Electron",
        "POS",
        "SaladLab",
        "Excel"
    ],
    "author": "hlhr202",
    "license": "Apache-2.0",
    "devDependencies": {
        "electron": "^1.4.1",
        "electron-packager": "^8.3.0"
    },
    "dependencies": {
        "babel-core": "^6.18.2",
        "babel-preset-stage-1": "^6.16.0",
        "babel-loader": "^6.2.8",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.16.0",
        "electron-json-storage": "^2.1.0",
        "moment": "^2.17.0",
        "node-schedule": "^1.2.0",
        "react": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-tap-event-plugin": "^2.0.1",
        "semantic-ui-css": "^2.2.4",
        "semantic-ui-react": "^0.61.4",
        "xlsx": "^0.8.0"
    }
}